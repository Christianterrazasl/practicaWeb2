<%- include('../components/header') %>

<div class="container">
    <h1>Personas Registradas</h1>
    <table class="table">
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Apellido</th>
                <th>Email</th>
                <th>Fecha de nacimiento</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.nombre %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</div>

<%- include('../components/footer') %>